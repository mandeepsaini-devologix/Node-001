<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="">
        List of Products
    </div>

    <% data.forEach(function(item) { %>
        
        <div class="">
            <span><%= item.id %></span> 
            <span> <%= item.name %> </span>
            <a href="/product/<%= item.slug %>">View Product</a>
        </div>


    <% }); %>



    
    


</body>
</html>